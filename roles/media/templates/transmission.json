{
  "umask": 2,
  "encryption": 1,
  "download-dir": "{{downloads}}",
  "watch-dir": "{{watch}}",
  "watch-dir-enabled": true,
  "trash-original-torrent-files": true,
  "incomplete-dir": "{{incomplete}}",
  "incomplete-dir-enabled": true,
  "idle-seeding-limit": 560,
  "idle-seeding-limit-enabled": true,
  "ratio-limit": "{{transmission.ratio}}",
  "ratio-limit-enabled": true,
  "rpc-enabled": true,
  "rpc-username": "{{transmission.user}}",
  "rpc-password": "{{transmission.password}}",
  "rpc-authentication-required": false,
  "rpc-whitelist": "192.168.*.*,127.0.0.1",
  "rpc-whitelist-enabled": true,
  "rpc-bind-address": "{{transmission.address}}",
  "rpc-port": {{transmission.port}},
  "script-torrent-done-enabled": true,
  "script-torrent-done-filename": "{{subs}}"
}
